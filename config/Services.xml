<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <services>
    
	<service id="configuration" class="Application\Config\Configuration"></service>
	
	<service id="cli" class="Application\CLI\CLI"></service>
	
	<service id="eventDispatcher" class="Symfony\Component\EventDispatcher\EventDispatcher"></service>
	
    <service id="settings" class="Application\Config\Settings">
      <argument type="string">config/settings.yml</argument>
    </service>
	
	<service id="schema" class="Application\Config\Schema">
      <argument type="string">config/schema.yml</argument>
    </service>
	
  </services>
</container>